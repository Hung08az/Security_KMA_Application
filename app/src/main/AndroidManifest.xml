<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kma_application"
    android:targetSandboxVersion="1">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/run"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/run"
        android:supportsRtl="true"
        android:theme="@style/Theme.KMA_Application"
        android:usesCleartextTraffic="true">
        <activity android:name=".child_health"></activity>
        <activity android:name=".class_health" />
        <activity android:name=".medicine" />
        <activity android:name=".health" />
        <activity android:name=".absent" />
        <activity android:name=".changePassword" />

        <activity android:name=".login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".contact" />
        <activity android:name=".user" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>